{
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 80,
  "proseWrap": "never",
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ],
  "semi": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "trailingComma": "all"
}
